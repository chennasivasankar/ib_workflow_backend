{
        "swagger": "2.0",
        "host": "127.0.0.1:8000",
        "basePath": "/api/ib_boards/",
        "info": {
            "version": "1.0.0",
            "title": "iB Boards API",
            "description": "Api for ib boards"
        },
        "schemes": [
            "http",
            "https"
        ],
        "consumes": [
            "application/json"
        ],
        "produces": [
            "application/json"
        ],
        "securityDefinitions": {
            "oauth": {
                "tokenUrl": "http://auth.ibtspl.com/oauth2/",
                "flow": "password",
                "scopes": {
                    "read": "read users",
                    "write": "create users",
                    "update": "update users",
                    "delete": "delete users"
                },
                "type": "oauth2"
            }
        },
        "security": [],
        "definitions": {
            "DefaultHttpExceptionFields": {
                "type": "object",
                "properties": {
                    "response": {
                        "type": "string"
                    },
                    "http_status_code": {
                        "type": "integer"
                    },
                    "res_status": {
                        "type": "string",
                        "enum": [
                            "DUPLICATE_TO_IDS",
                            "INVALID_INPUT_TO_IDS"
                        ]
                    }
                },
                "required": [
                    "response",
                    "http_status_code",
                    "res_status"
                ]
            },
            "Board": {
                "type": "object",
                "properties": {
                    "board_id": {
                        "type": "string"
                    },
                    "display_name": {
                        "type": "string"
                    }
                }
            }
        },
        "parameters": {
            "BoardsParameterOffset": {
                "description": "Offset",
                "in": "query",
                "name": "Offset",
                "required": true,
                "type": "integer"
            },
            "BoardsParameterLimit": {
                "description": "Limit",
                "in": "query",
                "name": "Limit",
                "required": true,
                "type": "integer"
            }
        },
        "responses": {
        },
        "paths": {
            "/boards/v1/": {
                "get": {
                    "summary": "Get Boards",
                    "description": "Returns a list of board details.",
                    "operationId": "get_boards",
                    "security": [
                        {
                            "oauth": [
                                "read"
                            ]
                        }
                    ],
                    "parameters": [
                        {
                            "$ref": "#/parameters/BoardsParameter"
                        }
                    ],
                    "responses": {
                        "200": {
                            "description": "A list of boards",
                            "schema": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/definitions/Board"
                                }
                            }
                        },
                        "401": {
                            "description": "Unauthorized",
                            "schema": {
                                "$ref": "#/definitions/DefaultHttpExceptionFields"
                            }
                        },
                        "403": {
                            "description": "Forbidden",
                            "schema": {
                                "$ref": "#/definitions/DefaultHttpExceptionFields"
                            }
                        },
                        "404": {
                            "description": "Not Found",
                            "schema": {
                                "$ref": "#/definitions/DefaultHttpExceptionFields"
                            }
                        }
                    }
                }
            }
        }
}
    